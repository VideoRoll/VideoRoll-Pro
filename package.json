{
  "name": "video-roll",
  "version": "2.0.0",
  "description": "Rotate, zoom, reposition, stretch, flip, tune, focus and filter videos",
  "author": "Gomi",
  "scripts": {
    "test": "jest",
    "watch:manifest-chromium": "cross-env BROWSER=\"chromium\" node scripts/hmr.js",
    "watch:manifest-firefox": "cross-env BROWSER=\"firefox\" node scripts/hmr.js",
    "watch:entry": "parcel watch src/lib/share.js src/inject/capture.html src/popup/index.html src/download/download.html src/player/player.html src/inject/index.ts src/background/index.ts src/options/index.html src/offscreen/offscreen.html --no-cache --no-hmr",
    "watch:dist": "pnpm run wait-for-dist && web-ext run --source-dir=dist --target chromium",
    "wait-for-dist": "wait-on dist/background/index.js dist/inject/index.js dist/manifest.json dist/offscreen/offscreen.js && sleep 1",
    "dev:uno": "unocss --watch --config uno.config.ts",
    "build:uno": "unocss --config uno.config.ts",
    "dev:chromium": "pnpm run clean && pnpm run copy:static && concurrently \"pnpm run watch:entry\" \"pnpm run watch:manifest-chromium\" \"pnpm run build:manifest-chromium\" \"pnpm run watch:dist\" \"pnpm run dev:uno\"",
    "dev:firefox": "pnpm run clean && pnpm run copy:static && concurrently \"pnpm run watch:entry\" \"pnpm run watch:manifest-firefox\" \"pnpm run build:manifest-firefox\" \"pnpm run watch:dist\" \"pnpm run dev:uno\"",
    "build:offscreen": "parcel build src/offscreen/offscreen.html --dist-dir dist/offscreen/ --no-source-maps --no-content-hash",
    "build:share": "parcel build src/lib/share.js --dist-dir dist/lib/ --no-source-maps --no-content-hash",
    "build:popup": "parcel build src/popup/index.html --dist-dir dist/popup/ --no-source-maps --no-content-hash",
    "build:popup-report": "pnpm run build:popup --reporter @parcel/reporter-bundle-analyzer",
    "build:inject": "parcel build src/inject/index.ts --dist-dir dist/inject --no-source-maps --no-content-hash",
    "build:inject-report": "pnpm run build:inject --reporter @parcel/reporter-bundle",
    "build:background": "parcel build src/background/index.ts --dist-dir dist/background --no-source-maps --no-content-hash",
    "build:background-report": "pnpm run build:background --reporter @parcel/reporter-bundle",
    "build:options": "parcel build src/options/index.html --dist-dir dist/options --no-source-maps --no-content-hash",
    "build:download": "parcel build src/download/download.html --dist-dir dist/download --no-source-maps --no-content-hash",
    "build:player": "parcel build src/player/player.html --dist-dir dist/player --no-source-maps --no-content-hash",
    "build:options-report": "pnpm run build:options --reporter @parcel/reporter-bundle",
    "build:manifest-chromium": "cross-env BROWSER=\"chromium\" node scripts/generateManifest.js",
    "build:manifest-firefox": "cross-env BROWSER=\"firefox\" node scripts/generateManifest.js",
    "build:html": "parcel build src/popup/index.html src/player/player.html src/offscreen/offscreen.html src/download/download.html src/options/index.html --dist-dir dist --no-source-maps --no-content-hash",
    "build": "pnpm run clean && pnpm run copy:static && pnpm run build:popup && pnpm run build:player && pnpm run build:download && pnpm run build:options && pnpm run build:offscreen && pnpm run build:inject && pnpm run build:background && pnpm run build:uno && pnpm run build:zip",
    "build:report": "pnpm run clean && pnpm run copy:static && pnpm run build:popup-report",
    "copy:static": "copyfiles -u 1 src/icons/* src/inject/capture.* dist && copyfiles _locales/*/*.json dist",
    "build:zip": "node scripts/build.js",
    "clean": "rimraf dist && rimraf chromiumExt.zip && rimraf firefoxExt.zip",
    "start": "pnpm run dev",
    "precommit": "pnpm run test"
  },
  "alias": {
    "src": "./src"
  },
  "@parcel/bundler-default": {
    "minBundles": 1,
    "minBundleSize": 1024,
    "maxParallelRequests": 20
  },
  "source": [
    "src/options/index.html",
    "src/popup/index.html",
    "src/inject/index.ts",
    "src/background/index.ts",
    "src/lib/share.js",
    "src/download/download.html",
    "src/player/player.html"
  ],
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gxy5202/video-roll.git"
  },
  "keywords": [
    "rotate video",
    "video rotater",
    "transform video",
    "html5",
    "video"
  ],
  "parcel-namer-hashless": {
    "mode": "all"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/gxy5202/video-roll/issues"
  },
  "homepage": "https://github.com/gxy5202/video-roll#readme",
  "dependencies": {
    "@ffmpeg/core": "^0.12.10",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@supabase/supabase-js": "^2.45.6",
    "@tabler/icons-vue": "^3.31.0",
    "@vant/touch-emulator": "^1.4.0",
    "@vant/use": "^1.6.0",
    "@vicons/ionicons5": "^0.11.0",
    "@vicons/tabler": "^0.13.0",
    "@xenova/transformers": "^2.17.2",
    "aes-decrypter": "^4.0.2",
    "audio-hacker": "^1.0.8",
    "dashjs": "^5.0.0",
    "easyqrcodejs": "^4.6.1",
    "file-saver": "^2.0.5",
    "floating-vue": "^5.2.2",
    "hls.js": "^1.5.20",
    "hotkeys-js": "^3.13.7",
    "lodash-es": "^4.17.21",
    "m3u8-parser": "^7.2.0",
    "mpd-parser": "^1.3.1",
    "nanoid": "^5.0.7",
    "plyr": "^3.7.8",
    "three": "^0.168.0",
    "vant": "4.8.5",
    "vue": "^3.5.13",
    "vue3-colorpicker": "^2.3.0",
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@parcel/config-default": "^2.15.2",
    "@parcel/plugin": "^2.15.2",
    "@parcel/reporter-bundle-analyzer": "^2.8.3",
    "@parcel/transformer-html": "^2.15.2",
    "@parcel/transformer-less": "^2.15.2",
    "@parcel/transformer-vue": "^2.15.2",
    "@parcel/validator-typescript": "^2.15.2",
    "@types/chrome": "^0.0.227",
    "@types/file-saver": "^2.0.7",
    "@types/jest": "^29.4.0",
    "@types/lodash-es": "^4.17.12",
    "@types/m3u8-parser": "^7.2.0",
    "@types/node": "^18.14.2",
    "@types/three": "^0.168.0",
    "@types/webextension-polyfill": "^0.10.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@unocss/cli": "^66.1.2",
    "@unocss/preset-attributify": "^66.1.2",
    "@unocss/preset-uno": "^66.1.2",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/compiler-sfc": "^3.2.47",
    "buffer": "^5.5.0",
    "chalk": "^5.2.0",
    "compressing": "^1.8.0",
    "concurrently": "^9.1.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "dirname-filename-esm": "^1.1.1",
    "eslint": "^8.35.0",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "less": "^4.1.3",
    "parcel": "^2.15.2",
    "parcel-namer-hashless": "^1.0.7",
    "process": "^0.11.10",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "unocss": "^66.1.2",
    "wait-on": "^8.0.1",
    "web-ext": "^8.7.1"
  }
}
